{
  "name": "@adhocteam/kongfig",
  "version": "2.1.4",
  "description": "A tool for Kong to allow declarative configuration.",
  "repository": "https://github.com/adhocteam/kongfig",
  "bin": {
    "kongfig": "./bin/kongfig"
  },
  "preferGlobal": true,
  "scripts": {
    "pretest": "npm run-script build",
    "build": "babel src --out-dir lib",
    "version": "npm run-script build",
    "postversion": "git push && git push --tags",
    "publish-patch": "npm version patch && npm publish",
    "test": "jest --testPathPattern=test/.*",
    "test-integration": "TEST_INTEGRATION_KONG_HOST=localhost:8001 jest --runInBand --testPathPattern=test-integration/.*",
    "local-install": "npm run build && npm install -g $PWD"
  },
  "author": "MyBuilder Ltd",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "colors": "^1.3.3",
    "commander": "^2.13.0",
    "deepdash": "^4.2.16",
    "invariant": "^2.2.4",
    "isomorphic-fetch": "^2.2.0",
    "js-yaml": "^3.13.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.remove": "^4.5.0",
    "minimist": "^1.2.0",
    "object-assign": "^4.0.1",
    "pad": "^2.0.3",
    "prettyjson": "^1.1.3",
    "semver": "^5.6.0",
    "socks-proxy-agent": "^4.0.1",
    "url": "^0.11.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.1.4",
    "expect.js": "^0.3.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/@babel/polyfill/dist/polyfill.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ]
  }
}
